; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "Character",
"language": "GDScript",
"path": "res://Character/Character.gd"
}, {
"base": "Control",
"class": "EndScreen",
"language": "GDScript",
"path": "res://Interface/EndScreen/EndScreen.gd"
}, {
"base": "",
"class": "GitAPI",
"language": "NativeScript",
"path": "res://git_api.gdns"
}, {
"base": "Node2D",
"class": "LevelController",
"language": "GDScript",
"path": "res://Levels/LevelController/LevelController.gd"
}, {
"base": "Node",
"class": "SceneLoader",
"language": "GDScript",
"path": "res://Levels/SceneLoader/SceneLoader.gd"
} ]
_global_script_class_icons={
"Character": "",
"EndScreen": "",
"GitAPI": "",
"LevelController": "",
"SceneLoader": ""
}

[application]

config/name="Balance Game"
run/main_scene="res://Levels/SceneLoader/SceneLoader.tscn"
config/icon="res://icon.png"

[autoload]

AsyncLoader="*res://AutoLoad/AsyncLoader.gd"

[debug]

gdscript/warnings/return_value_discarded=false

[display]

window/size/height=1024
window/size/test_width=1920
window/size/test_height=1080
window/dpi/allow_hidpi=true
window/stretch/mode="2d"
window/stretch/aspect="expand"

[gdnative]

singletons=[ "res://git_api.gdnlib" ]

[gui]

theme/use_hidpi=true
theme/custom="res://Themes/DefaultTheme.tres"

[input]

reset_level={
"deadzone": 0.5,
"events": [ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":82,"unicode":0,"echo":false,"script":null)
 ]
}
character_compress={
"deadzone": 0.5,
"events": [ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":32,"unicode":0,"echo":false,"script":null)
 ]
}
toggle_fullscreen={
"deadzone": 0.5,
"events": [ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":70,"unicode":0,"echo":false,"script":null)
 ]
}
exit_fullscreen={
